<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:cuschatarreq="http://www.ericsson.com/esb/message/customer/changeTariffRequest/v1.0" targetNamespace="http://www.ericsson.com/esb/message/customer/changeTariffRequest/v1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="changeTariffRequest" type="cuschatarreq:changeTariffRequestType" />
