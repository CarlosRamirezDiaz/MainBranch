    <xsd:sequence>
      <xsd:element name="changeTariff" type="cuschatarreq:CustomerTariff" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CustomerTariff">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="entityType" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="entityId" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="tariffId" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="chargeExponent" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="oldTariffId" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="referenceNumber" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="eventSource" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>